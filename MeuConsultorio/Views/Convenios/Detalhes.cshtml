@model MeuConsultorio.Models.Convenio

@{
    ViewBag.Title = "Detalhes";
}

<h2>Detalhes</h2>

<fieldset>
    <legend>Convenio</legend>

    <div class="display-label">Codigo</div>
    <div class="display-field">@Model.Codigo</div>

    <div class="display-label">Descricao</div>
    <div class="display-field">@Model.Descricao</div>

    <div class="display-label">Fatura</div>
    <div class="display-field">@Model.Fatura</div>
</fieldset>
<p>
    @Html.ActionLink("Editar", "Editar", new { id=Model.ConvenioId }) |
    @Html.ActionLink("Voltar para lista", "Indice")
</p>


