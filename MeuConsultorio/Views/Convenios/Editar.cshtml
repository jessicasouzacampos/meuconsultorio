@model MeuConsultorio.Models.Convenio

@{
    ViewBag.Title = "Editar";
}

<h2>Editar</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Convenio</legend>

        @Html.HiddenFor(model => model.ConvenioId)
		@Html.Partial("_CriarOuEditar", Model)

        <p>
            <input type="submit" value="Salvar" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Voltar para lista", "Indice")
</div>
